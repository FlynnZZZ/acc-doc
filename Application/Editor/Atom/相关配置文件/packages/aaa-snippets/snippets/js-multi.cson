# js & vue 
'.source.js,.text.html.vue':
  # event name   
  'JM touchstart  触摸开始':
    'prefix': 'touchstart-eventName'
    'body': 'touchstart'
  'JM touchend  触摸结束':
    'prefix': 'touchend-eventName'
    'body': 'touchend'
  'JM touchmove  滑动':
    'prefix': 'touchmove-eventName'
    'body': 'touchmove'
  'JM focus  获得焦点':
    'prefix': 'focus-eventName'
    'body': 'focus'
  'JM blur  失去焦点':
    'prefix': 'blur-eventName'
    'body': 'blur'
  # Vue 
  'JM Vue.extend({});  扩展Vue构造器':
    'prefix': 'vue-extend'
    'body': """Vue.extend({
        $1
      });""" 
  'JM Vue.component("c-name",{})  全局组件':
    'prefix': 'vue-component'
    'body': """Vue.component('c-name',{
      template: ''
      ,props: []
      ,props: {
        prop0: {
          default: xxx
          default: function () {
            return 
          }
          ,type: Number 
          ,required: bol  
          ,validator: function (val) { 
            return bol;
          }
        }
        ,prop1: Number 
        ,prop1: [Number,String] 
      }
      ,data: function(){
        return {
          
        }
      }
      ,created: function(){
      }
    });""" 
  'JM Vue.component("c-name",foo)  异步全局组件':
    'prefix': 'vue-component-async'
    'body': """Vue.component("c-name",function(rs,rj){
      rs({
        template: ''
        ,props: []
        ,props: {
          prop0: {
            default: xxx
            default: function () {
              return 
            }
            ,type: Number 
            ,required: bol  
            ,validator: function (val) { 
              return bol;
            }
          }
          ,prop1: Number 
          ,prop1: [Number,String] 
        }
        ,data: function(){
          return {
            
          }
        }
        ,created: function(){
        }
      })
    });""" 
  'JM Vue.directive("xx-xx",{});  全局指令':
    'prefix': 'vueDirective'
    'body': """Vue.directive('xxx-xxx', {
      bind : function(el,binds,vn){
        // binds{name,value,expression,arg,modifiers}
      }
      ,inserted : function(el,binds,vn){
        
      }
      ,update : function(el,binds,vn,oVn){
        // binds{name,value,expression,arg,modifiers,oldValue}
      }
      ,componentUpdated : function(el,binds,vn,oVn){
        // binds{name,value,expression,arg,modifiers,oldValue}
      }
      ,unbind : function(el,binds,vn,oVn){
        
      }
    });""" 
  'JM Vue.filter("xxx",foo);  全局过滤器':
    'prefix': 'vue-filter'
    'body': """Vue.filter('xxx',function(val,arg2,arg3){
        $1 
      });""" 
  'JM Vue.use($1)  注册插件':
    'prefix': 'vue-use'
    'body': 'Vue.use($1)'
  'JM new Vue({$1});  实例化Vue':
    'prefix': 'new-vue'
    'body': """new Vue({
        el: '#xxx'
        ,data: {}
        ,computed: {}
        ,watch: {}
        ,methods: {}
        ,created: function (){
          var that = this;
        }
        ,mounted: function (){
          var that = this;
          this.$nextTick(function () { });
          
        }
      });""" 
  'JM props  KW,传递属性':
    'prefix': 'props-vue'
    'body': """props""" 
  'JM propsData  KW,传递属性数据':
    'prefix': 'propsData-vue'
    'body': """propsData""" 
  'JM computed  KW,计算值':
    'prefix': 'computed-vue'
    'body': """computed""" 
  'JM watch  KW,观察数据':
    'prefix': 'watch-vue'
    'body': """watch""" 
  'JM methods  KW,方法':
    'prefix': 'methods-vue'
    'body': """methods""" 
  'JM filters  KW,过滤':
    'prefix': 'filters-vue'
    'body': """filters""" 
  'JM directives  KW,指令':
    'prefix': 'directives-vue'
    'body': """directives""" 
  'JM mixins  KW,混合':
    'prefix': 'mixins-vue'
    'body': """mixins""" 
  'JM components  KW,子组件':
    'prefix': 'components-vue'
    'body': """components""" 
  'JM beforeCreate  实例刚创建':
    'prefix': 'beforeCreate-vue'
    'body': """beforeCreate""" 
  'JM created  实例创建完毕':
    'prefix': 'created-vue'
    'body': """created""" 
  'JM beforeMount  DOM渲染前':
    'prefix': 'beforeMount-vue'
    'body': """beforeMount""" 
  'JM mounted  DOM渲染完毕':
    'prefix': 'mounted-vue'
    'body': """mounted""" 
  'JM beforeUpdate  更新前':
    'prefix': 'beforeUpdate-vue'
    'body': """beforeUpdate""" 
  'JM updated  数据模型更新':
    'prefix': 'updated-vue'
    'body': """updated""" 
  'JM activated  组件被激活时':
    'prefix': 'activated-vue'
    'body': """activated""" 
  'JM deactivated  组件被移除时':
    'prefix': 'deactivated-vue'
    'body': """deactivated""" 
  'JM beforeDestroy  销毁前':
    'prefix': 'beforeDestroy-vue'
    'body': """beforeDestroy""" 
  'JM destroyed  销毁观察、组件及事件':
    'prefix': 'destroyed-vue'
    'body': """destroyed""" 
  'JM .$set(obj,key,val)  更新数据':
    'prefix': 'set-vue'
    'body': """$set(obj,key,val)""" 
  'JM .$emit("eventName",data)  触发事件':
    'prefix': 'emit-vue'
    'body': """$emit('eventName',data)""" 
  'JM .$on("eventName",function (){})  监听事件':
    'prefix': 'on-vue'
    'body': """$on('eventName',function (data){
      $1
    })""" 
  'JM .$refs  ':
    'prefix': 'refs-vue'
    'body': """$refs""" 
  # vue插件 
  'JM new VueRouter();  实例化路由':
    'prefix': 'new-VueRouter'
    'body': """new VueRouter({
        routes : $1
      });""" 
  'JM new Vuex.Store({})  实例化数据中心':
    'prefix': 'new Vuex.store'
    'body': """new Vuex.store({ 
      state: { 
        aoo: 123  
      }
      ,mutations: {  
        foo: function(state,data){  
        }
      }
      ,getters: {  
        boo: function(state,getters){
          return state.xx;
        }
      }
      ,actions: {   
        goo: function(context,data){  
        }
      }
      ,modules: {  
      }
    })""" 
  'JM redirect  KW,重定向':
    'prefix': 'redirect-KW-vue'
    'body': """redirect""" 
  'JM namespaced  KW,命名空间':
    'prefix': 'namespaced-KW-vue'
    'body': """namespaced""" 
  'JM mapState  FN,状态绑定函数':
    'prefix': 'mapState-FN-vue'
    'body': """mapState""" 
  'JM mapMutations  FN,commit绑定函数':
    'prefix': 'mapMutations-FN-vue'
    'body': """mapMutations""" 
  'JM mapGetters  FN,getters绑定函数':
    'prefix': 'mapGetters-FN-vue'
    'body': """mapGetters""" 
  'JM mapActions  FN,dispatch绑定函数':
    'prefix': 'mapActions-FN-vue'
    'body': """mapActions""" 
  'JM .$router  路由实例':
    'prefix': 'router-vue'
    'body': """$router""" 
  'JM .$store  数据中心':
    'prefix': 'store-vue'
    'body': """$store""" 
  'JM .$store.state.xx  原始数据':
    'prefix': 'store.state-vue'
    'body': """$store.state""" 
  'JM .$store.getters.xx  组件中使用数据':
    'prefix': 'store.getters-vue'
    'body': """$store.getters""" 
  'JM .$store.commit("foo",data)  执行mutations的方法‹操作state›':
    'prefix': 'store.commit-vue'
    'body': """$store.commit('foo',data)""" 
  'JM commit("foo",data)  执行mutations的方法‹操作state›':
    'prefix': 'commit-vue'
    'body': """commit('foo',data)""" 
  'JM .$store.dispatch("goo",data)  执行actions的方法‹操作mutations›':
    'prefix': 'store.dispatch-vue'
    'body': """$store.dispatch('goo',data)""" 
  'JM dispatch("goo",data)  执行actions的方法‹操作mutations›':
    'prefix': 'dispatch-vue'
    'body': """dispatch('goo',data)""" 
  'JM .$http.get().then  GET请求':
    'prefix': 'http.get-vue'
    'body': """$http.get('${1:_url_}',{})  // 某接口
    .then(function(back){
      console.info('_url_','某接口',back.statusText);
    }
    ,function(err){
      console.warn('_url_','某接口',err); 
    }) """ 
  'JM .$http.post().then  POST请求':
    'prefix': 'http.post-vue'
    'body': """$http.post('${1:_url_}',{})  // 某接口
    .then(function(back){
      console.info('_url_','某接口',back.statusText);
    }
    ,function(error){
      console.warn('_url_','某接口',err); 
    })""" 



